{
  "private": true,
  "scripts": {
    "dev": "next",
    "dev-graphql": "concurrently next \"graphql-codegen --watch\"",
    "build": "yarn lang && yarn graphql:gen && next build",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "lint": "next lint",
    "test": "jest --watch",
    "test:ci": "yarn type-check && yarn lint && jest",
    "format": "prettier --write **/*.{ts,tsx}",
    "lang:extract": "formatjs extract {pages,components}/**/*.{ts,tsx} --format simple --id-interpolation-pattern [sha512:contenthash:base64:6] --out-file lang/en.json",
    "lang:compile": "formatjs compile-folder --ast --format simple lang lang/generated",
    "lang": "yarn lang:extract && yarn lang:compile",
    "seed": "ts-node database/seeder/cli",
    "graphql:gen": "ts-node graphql/importGenerator && graphql-codegen"
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@graphql-codegen/add": "^3.1.1",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/graphql-modules-preset": "^2.3.7",
    "@graphql-codegen/typed-document-node": "^2.2.7",
    "@graphql-codegen/typescript": "^2.4.7",
    "@graphql-codegen/typescript-operations": "^2.3.4",
    "@graphql-codegen/typescript-react-apollo": "^3.2.10",
    "@graphql-codegen/typescript-resolvers": "^2.5.4",
    "@graphql-tools/load-files": "^6.5.3",
    "@graphql-tools/merge": "^8.2.3",
    "@next-auth/mongodb-adapter": "^1.0.1",
    "@styled-icons/fa-brands": "^10.38.0",
    "@styled-icons/fa-solid": "^10.34.0",
    "@types/luxon": "^2.0.4",
    "@types/node-cache": "^4.2.5",
    "apollo-server-micro": "^3.6.7",
    "axios": "^0.26.1",
    "graphql": "^16.3.0",
    "graphql-tools": "^8.2.0",
    "lodash": "^4.17.21",
    "luxon": "^2.0.2",
    "md5": "^2.3.0",
    "micro": "^9.3.4",
    "micro-cors": "^0.1.1",
    "mongoose": "^6.2.6",
    "next": "12",
    "next-auth": "^4.3.3",
    "node-cache": "^5.1.2",
    "polished": "^4.1.4",
    "react": "^18.2.0",
    "react-click-away-listener": "^2.1.0",
    "react-dom": "^18.2.0",
    "react-intl": "^5.20.12",
    "react-is": "^17.0.2",
    "react-tooltip": "^4.2.21",
    "sharp": "^0.30.5",
    "slugify": "^1.6.5",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.0.0-beta.0",
    "@formatjs/cli": "^4.8.2",
    "@next/bundle-analyzer": "^12.1.0",
    "@next/eslint-plugin-next": "^12.2.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/glob": "^7.2.0",
    "@types/lodash": "^4.14.179",
    "@types/md5": "^2.3.2",
    "@types/micro-cors": "^0.1.2",
    "@types/node": "15.0.0",
    "@types/prettier": "^2.4.4",
    "@types/react": "^17.0.33",
    "@types/react-dom": "17.0.3",
    "@types/styled-components": "^5.1.24",
    "concurrently": "^7.0.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.20.0",
    "eslint-config-next": "12.2.2",
    "glob": "^7.2.0",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.0",
    "typescript": "4.4.2"
  }
}
